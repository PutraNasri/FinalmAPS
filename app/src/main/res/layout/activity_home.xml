<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="maps.elektro.com.finalmaps.home"
    android:focusable="false"
    android:background="@drawable/bbg"
    >


    <TextView android:text="@string/hello_world" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView" />





    <ImageView
        android:layout_width="80dp"
        android:layout_height="70dp"
        android:id="@+id/imageView"
        android:src="@drawable/logoaceh"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Aceh Travel List"
        android:id="@+id/button2"
        android:layout_below="@+id/imageView"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        android:onClick="onClicktombol1"
        android:textStyle="bold"
        android:background="@color/bright_foreground_disabled_material_dark"
        android:textColor="#000000" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Go To Maps"
        android:id="@+id/button3"
        android:layout_below="@+id/button2"
        android:layout_centerHorizontal="true"
        android:textColor="#000000"
        android:textStyle="bold"
        android:background="@color/bright_foreground_disabled_material_dark"
        android:onClick="onClicktombol2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Event"
        android:id="@+id/button4"
        android:layout_below="@+id/button3"
        android:layout_centerHorizontal="true"
        android:background="@color/bright_foreground_disabled_material_dark"
        android:textStyle="bold"
        android:textColor="#000000" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="About"
        android:id="@+id/button6"
        android:background="@color/bright_foreground_disabled_material_dark"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_below="@+id/button4"
        android:layout_centerHorizontal="true"
        android:onClick="onClicktombol4" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button7"
        android:background="@color/bright_foreground_disabled_material_dark"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_below="@+id/button6"
        android:layout_centerHorizontal="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button8"
        android:background="@color/bright_foreground_disabled_material_dark"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_below="@+id/button7"
        android:layout_centerHorizontal="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/button9"
        android:background="@color/bright_foreground_disabled_material_dark"
        android:textStyle="bold"
        android:textColor="#000000"
        android:layout_below="@+id/button8"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
